<!DOCTYPE xml>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>{{seo.title}}</title>
    <link>{{url}}/rss</link>
    <atom:link href="{{url}}/rss" rel="self" type="application/rss+xml" />
    <description>{{seo.description}}</description>
    <generator>https://sheet-posting.glitch.me</generator>
    {{#each posts}}
      <item>
        <guid>
          {{url}}/{{this.slug}}
        </guid>
        <title>
          {{this.title}}
        </title>
        <link>
          {{this.source}}
        </link>
        <image>
          <url>{{this.image}}</url>
        </image>
        <pubDate>
          {{this.utcDate}}
        </pubDate>
        <description>
          <![CDATA[{{{this.html}}}]]>
        </description>
      </item>
    {{/each}}
  </channel>
</rss>